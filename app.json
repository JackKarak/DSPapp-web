{
  "expo": {
    "name": "The DSP App",
    "slug": "dsp-app",
    "version": "1.0.1",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dspapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deltasigmapi.app",
      "buildNumber": "4",
      "deploymentTarget": "15.1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The DSP App needs access to your photo library to allow you to upload profile pictures and event photos for fraternity activities and member identification.",
        "NSCameraUsageDescription": "The DSP App needs camera access to allow you to take photos for your profile, event documentation, and submission of required materials.",
        "NSLocationWhenInUseUsageDescription": "The DSP App uses your location only when you're using the app to help you find nearby fraternity events and provide location-based services for event check-ins.",
        "NSDocumentPickerUsageDescription": "The DSP App needs access to your documents to allow you to upload files for scholarship submissions, event materials, and other fraternity-related documentation.",
        "NSFaceIDUsageDescription": "The DSP App uses Face ID to securely authenticate your identity and keep your fraternity account safe. You can always use your password instead.",
        "NSCalendarsUsageDescription": "The DSP App can add approved fraternity events to your calendar so you never miss important activities and meetings.",
        "NSRemindersUsageDescription": "The DSP App can create reminders for upcoming fraternity events and deadlines.",
        "NSContactsUsageDescription": "The DSP App accesses your contacts only to help you find other fraternity members and connect with brothers.",
        "NSPrivacyPolicyURL": "https://sites.google.com/terpmail.umd.edu/dspapp/privacy-policy?authuser=0",
        "CFBundleLocalizations": ["en"],
        "ITSAppUsesNonExemptEncryption": false,
        "NSPrivacyTracking": false,
        "NSPrivacyTrackingDomains": [],
        "NSPrivacyCollectedDataTypes": [
          {
            "NSPrivacyCollectedDataType": "NSPrivacyCollectedDataTypeEmailAddress",
            "NSPrivacyCollectedDataTypeLinked": true,
            "NSPrivacyCollectedDataTypeTracking": false,
            "NSPrivacyCollectedDataTypePurposes": ["NSPrivacyCollectedDataTypePurposeAppFunctionality"]
          },
          {
            "NSPrivacyCollectedDataType": "NSPrivacyCollectedDataTypeName",
            "NSPrivacyCollectedDataTypeLinked": true,
            "NSPrivacyCollectedDataTypeTracking": false,
            "NSPrivacyCollectedDataTypePurposes": ["NSPrivacyCollectedDataTypePurposeAppFunctionality"]
          },
          {
            "NSPrivacyCollectedDataType": "NSPrivacyCollectedDataTypeUserID",
            "NSPrivacyCollectedDataTypeLinked": true,
            "NSPrivacyCollectedDataTypeTracking": false,
            "NSPrivacyCollectedDataTypePurposes": ["NSPrivacyCollectedDataTypePurposeAppFunctionality"]
          }
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:deltasigmapi.org"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.deltasigmapi.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "privacy": "public",
    "jsEngine": "hermes",
    "extra": {
      "privacyPolicyUrl": "https://sites.google.com/terpmail.umd.edu/dspapp/privacy-policy?authuser=0",
      "termsOfServiceUrl": "https://sites.google.com/terpmail.umd.edu/dspapp/terms-of-service?authuser=0"
    }
  }
}
