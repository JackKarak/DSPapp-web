{
  "expo": {
    "name": "The DSP App",
    "slug": "dsp-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dspapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deltasigmapi.app",
      "buildNumber": "1",
      "deploymentTarget": "13.0",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to allow you to upload profile pictures and event photos.",
        "NSCameraUsageDescription": "This app accesses your camera to allow you to take photos for your profile and events.",
        "NSLocationWhenInUseUsageDescription": "This app uses location services to help find event locations and provide location-based features.",
        "NSDocumentPickerUsageDescription": "This app accesses documents to allow you to upload files for your profile and submissions."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.deltasigmapi.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": false,
    "privacy": "unlisted"
  }
}
